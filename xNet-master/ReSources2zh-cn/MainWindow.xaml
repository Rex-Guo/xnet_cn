<Window x:Class="ReSources2zh_cn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReSources2zh_cn"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>

        <!--<StackPanel >        </StackPanel>-->
        <StackPanel>
            <Canvas>
                <Border BorderThickness="1" ></Border>
                <WebBrowser x:Name="webView" Height="266"   Width="397" Canvas.Left="4" Canvas.Top="49" LoadCompleted="webView_LoadCompleted"/>
                <Button x:Name="Open" Content="OPen" Canvas.Left="426" Canvas.Top="27" Width="75" RenderTransformOrigin="0.484,0.416" Height="31" Click="Refresh_Click"/>
                <TextBox x:Name="url" Height="23" TextWrapping="Wrap" Text="http://fanyi.youdao.com/" Canvas.Left="11" Canvas.Top="16" Width="389"/>
                <Button x:Name="Cancel" Content="Cancel" RenderTransformOrigin="0.484,0.416" Height="31" Canvas.Left="429" Canvas.Top="70" Width="71" Click="Cancel_Click" />
    <Label x:Name="label" Content="Label" Canvas.Left="432" Canvas.Top="111"/>
            </Canvas>
        


        </StackPanel>



    </Grid>



</Window>
